compositor_node MyOwnWorkspace_Node
{
    in 0 renderwindow
    target renderwindow
    {
        pass clear
        {
            colour_value 0.6 0 0.6 1
        }
        pass custom MYGUI
        {
        }
        pass render_scene
        {
            rq_first    0
            rq_last     max
        }
    }
}
workspace MyOwnWorkspace
{
    connect_output MyOwnWorkspace_Node 0
}
